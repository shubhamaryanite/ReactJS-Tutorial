//direct function
function printmyname(name)
{
  console.log(name);
}
printmyname("shubham");


//arrow function
const print=(name)=>{
  console.log(name);
}

print("sachdeva");


//arrow function returning
const multiply=(number1, number2)=>{
  return number1 * number2;
}

console.log(multiply(3,5));
